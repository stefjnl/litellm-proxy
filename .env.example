# LiteLLM Proxy Environment Variables
# Copy to .env and fill in your values

# Master key for proxy authentication (clients use this to authenticate)
LITELLM_MASTER_KEY=sk-your-master-key

# Salt key for LiteLLM internal use
LITELLM_SALT_KEY=sk-your-salt-key

# NanoGPT API Key (proxy authenticates with NanoGPT using this)
NANOGPT_API_KEY=your-nanogpt-api-key
